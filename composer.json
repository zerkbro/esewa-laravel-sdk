{
  "name": "cixware/esewa-php-sdk",
  "version": "2.6.0",
  "type": "library",
  "description": "eSewa payment gateway integration in PHP.",
  "homepage": "https://github.com/cixware/esewa-php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Cixware",
      "email": "support@cixware.io",
      "homepage": "https://cixware.io"
    },
    {
      "name": "Madan Sapkota",
      "email": "contact+github@madansapkota.com",
      "homepage": "https://madansapkota.com"
    }
  ],
  "keywords": [
    "php",
    "api",
    "esewa",
    "payment",
    "gateway",
    "nepal"
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.42",
    "phpunit/phpunit": "^9.6.15",
    "rector/rector": "^0.18.13"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cixware\\Esewa\\EsewaServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Cixware\\Esewa\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
