{
  "name": "zerkbro/esewa-laravel-sdk",
  "version": "1.0",
  "type": "library",
  "description": "eSewa payment gateway integration in Laravel.",
  "homepage": "https://github.com/zerkbro/esewa-laravel-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Subash Sharma",
      "email": "contact+zerkxubas@gmail.com",
      "homepage": "https://zerkbro.com"
    }
  ],
  "keywords": [
    "php",
    "api",
    "esewa",
    "payment",
    "gateway",
    "nepal"
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.42",
    "phpunit/phpunit": "^9.6.15",
    "rector/rector": "^0.18.13"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZerkBro\\Laravel\\EsewaServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "ZerkBro\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
